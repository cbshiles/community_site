package com.ah.engine.util;

import com.ah.engine.model.inbound.GeneralMembership;

public abstract class EligibleMember {

    public GeneralMembership gm;
    public boolean valid;
    public String payer;

    public EligibleMember(GeneralMembership gm){
	this.gm = gm;
	valid = true;
	payer = null;
    }
    
    public EligibleMember(GeneralMembership gm, boolean valid, String payer){
	this.gm = gm;
	this.valid = valid;
	this.payer =payer;
    }

    public String getMemId(){
	return gm.getMemId();
    }
    
    public String createOutputLine(String measure, String payOutput);
}
